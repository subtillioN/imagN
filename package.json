{
  "name": "imagn",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development vite",
    "build": "NODE_ENV=production tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch",
    "test:update": "jest -u",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx,css,md}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,css,md}'",
    "analyze-bundle": "vite build --analyze",
    "prepare": "husky install",
    "context:load": "node .cursor/scripts/context-loader.js",
    "context:save": "node .cursor/scripts/context-saver.js",
    "context:update-vectors": "node .cursor/scripts/vector-generator.js",
    "context:update": "node .cursor/scripts/context-updater.js",
    "context:list": "node -e \"const fs=require('fs');const path=require('path');const emb=JSON.parse(fs.readFileSync(path.resolve(__dirname, 'context/vector-embeddings.json')));console.log('Available contexts:\\n' + Object.entries(emb.contexts).map(([k,v]) => `- ${k}: ${v.name} - ${v.description}`).join('\\n'))\""
  },
  "dependencies": {
    "@cycle/core": "^6.0.3",
    "@cycle/dom": "^23.1.0",
    "@cycle/react": "^2.10.0",
    "@cycle/run": "^5.7.0",
    "@cycle/state": "^1.7.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.5",
    "@mui/material": "^6.4.5",
    "@mui/styled-engine": "^6.0.0",
    "@mui/system": "^5.15.9",
    "@types/d3": "^7.4.3",
    "@types/react-dom": "^19.0.4",
    "callbag-basics": "^4.0.0",
    "callbag-from-obs": "^1.2.0",
    "callbag-start-with": "^3.1.0",
    "callbag-subscribe": "^1.5.1",
    "callbag-to-obs": "^1.0.0",
    "d3": "^7.9.0",
    "recharts": "^2.15.1",
    "rxjs": "^7.8.2",
    "xstream": "^11.14.0",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.26.9",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.26.0",
    "@babel/runtime": "^7.26.9",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/jest-axe": "^3.5.9",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^18.19.76",
    "@types/react": "^19.0.10",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitejs/plugin-react": "^4.3.4",
    "babel-jest": "^29.7.0",
    "callbag-subject": "^2.1.0",
    "chalk": "^5.4.1",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-functional": "^5.0.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-axe": "^9.0.0",
    "jest-environment-jsdom": "^29.4.2",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^2.8.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-refresh": "^0.16.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "vite": "^4.5.9"
  },
  "description": "Image workflow management tool",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/subtillioN/imagN.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/subtillioN/imagN/issues"
  },
  "homepage": "https://github.com/subtillioN/imagN#readme",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "type": "module"
}
